{
    "references" :[
        {
            "path" : "../tsconfig.json"
        }
    ],
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2015",
      "inlineSources": true,
      "lib": [ "es6" ],
      "outDir": "out",
      "sourceMap": true,
      "declaration": true,
      "preserveConstEnums": true,
      "strict": true,
      "typeRoots": [
        "./node_modules/@types"
      ],
      "types": [
        "node"
      ]
    },
    "exclude": [
      "node_modules",
      "./out"
    ]
  }
  
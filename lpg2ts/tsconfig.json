{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": [ "es6" ],

    "sourceMap": true,
    "experimentalDecorators": true,
    "declaration": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": false,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "alwaysStrict": true
    
  },
  "exclude": [
    "node_modules"
  ]
}
